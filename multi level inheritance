#include <iostream>
#include <string>
using namespace std;

class Person {
public:
    string name;
    int age;
      
    };
    class Student : public Person {
public:
        int rollno;
        
        };
   class gradStudent : public Student{
       public:
       string researcharea;
   } ;    
        
int main() {
  gradStudent s1;
  s1.name = "utkrisht";
  s1.age = 23;
  s1.rollno = 11212649;
  s1.researcharea = "Cs";
  cout<<s1.name<<endl;
  cout<<s1.researcharea<<endl;
   return 0;
}
